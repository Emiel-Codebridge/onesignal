{
    "name": "laravel-notification-channels/onesignal",
    "description": "OneSignal Notifications driver",
    "homepage": "https://github.com/Emiel-Codebridge/onesignal",
    "license": "MIT",
    "authors": [
        {
            "name": "Marcel Pociot",
            "email": "hello@marcelpociot.com",
            "homepage": "http://marcelpociot.com"
        },
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be"
        },
        {
            "name": "Emiel Blonk",
            "email": "emiel@codebridge.nl",
            "homepage": "https://codebridge.nl"
        }
    ],
    "require": {
        "php": "^7.2.0",
        "berkayk/onesignal-laravel": "^1.0.0",
        "illuminate/notifications": "5.*|^6.0",
        "illuminate/support": "5.*|^6.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\OneSignal\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\OneSignal\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "NotificationChannels\\OneSignal\\OneSignalServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
